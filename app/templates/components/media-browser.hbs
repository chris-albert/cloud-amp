<div class="row media-browser-section">
    {{browser-table-keys
    items=artistsKeys
    headers=artistColumns
    action="artistClicked"
    doubleClick="artistDoubleClicked"
    classNames="col-md-6"
    }}
    {{browser-table
    items=albums
    headers=albumHeaders
    action="albumClicked"
    doubleClick="albumDoubleClicked"
    classNames="col-md-6"
    }}
</div>
<div class="row media-browser-section">
    {{browser-table
    items=tracks
    headers=trackHeaders
    action="trackClicked"
    doubleClick="trackDoubleClicked"
    classNames="col-md-12"
    }}
</div>
